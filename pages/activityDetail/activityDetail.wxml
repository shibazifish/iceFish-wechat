<!--pages/activityDetail/activityDetail.wxml-->
<view>
    <view class="goods-info">
          <view class="c">
            <text class="name">{{prize.goods_name}}</text>
            <text class="desc">{{prize.goods_desc}}</text>
            <text class="desc">{{prize.goods_detail}}</text>
            <text class="price">￥:{{prize.goods_price}}元 {{prize.goods_ice}}块</text>
          </view>
      </view>

      <view>
        <button bindtap="onEnter">立即兑换</button>
      </view>
      <view>{{exchangeResult}}</view>
</view>
